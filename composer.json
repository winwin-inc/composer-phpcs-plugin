{
    "description": "Composer plugin for code style checking before commit using php-cs-fixer",
    "name": "winwin/composer-phpcs-plugin",
    "license": "MIT",
    "type": "composer-plugin",
    "require": {
        "php": "^7.2||^8.0",
        "composer-plugin-api": "^1.0||^2.0",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "require-dev": {
        "composer/composer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "winwin\\composer\\": "src/"
        }
    },
    "extra": {
        "class": "winwin\\composer\\PhpcsPlugin"
    }
}
